    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Controls and Movement
//Get player input
key_right = keyboard_check(ord('D'));
key_left = keyboard_check(ord('A'));
key_down = keyboard_check(ord('S'));
key_jump = keyboard_check_pressed(vk_space);
key_escape = keyboard_check_pressed(vk_escape);

// React to player input
// Inertia Mechanic
if (ifledging = false)
{
    if (hspeed &lt; maxmovespeed)
    {
        if (key_right = 1)
        {
            motion_add(0,movespeed)
        }
    }
    if (hspeed &gt; -maxmovespeed)
    {
        if (key_left = 1)
        {
            motion_add(0,-movespeed)
        }
    }
    if (hspeed &gt;= maxmovespeed)
    {
        if (key_right = 1)
        {
            motion_add(0,-movespeed)
        }
    }
    if (hspeed &lt;= -maxmovespeed)
    {
         if (key_left = 1)
        {
            motion_add(0,movespeed)
        }
    }
    if (key_right = 0)
    {
        if (key_left = 0)
        {
            if (hspeed &gt; 0)
            {
                motion_add(0,-movespeed)
            }
            if (hspeed &lt; 0)
            {
                motion_add(0,movespeed)
            }
            if (hspeed = 0)
            {
                motion_set(0,0)
            }
        }
    }
 }   
//Sprint
if (keyboard_check(vk_shift) &amp;&amp; global.stamina &gt;0)
    {
    maxmovespeed = 2;
    global.stamina = global.stamina- 0.1
    }
    else
    {
    maxmovespeed = 1;
    if (global.stamina &lt; maxstamina)
    {
        global.stamina = global.stamina + 0.05
    }
}
//Jump Mechanic
if (vsp &lt; 10) vsp += grav;
if (place_meeting(x,y+1,obj_wall))
{
    if (key_jump) 
    {
    vsp = -jumpspeed
    instance_create (x,y,obj_ledgegrab)
    }
}


//Dodge

if (keyboard_check_pressed(vk_alt)&amp;&amp; keyboard_check(ord('D')))
    {
        instance_destroy();
        instance_create(x,y,obj_player_roll);
        global.face = 1;
        is_dodging = true;
        
    }
else {
    is_dodging = false;
}
if (keyboard_check_pressed(vk_alt)&amp;&amp; keyboard_check(ord('A')))
    {
        instance_destroy();
        instance_create(x,y,obj_player_roll);
        global.face = 0;
        is_dodging = true;
        
    }
else {
    is_dodging = false;
}

//Pathing Activation
if (distance_to_object(instance_nearest(x, y, obj_pathing)) &lt; 64) {
    global.pathing_activate = true;
}
else if (distance_to_object(instance_nearest(x, y, obj_pathing)) &gt; 64) {
    global.pathing_activate = false;
}
    
//Horizontal Collision
if (place_meeting(x + hspeed,y,obj_wall))
{
    while(!place_meeting(x + sign(hspeed),y,obj_wall))
    {
        x += sign(hspeed);
    }
    hspeed = 0;
}
x += hsp;
//Vertical Collision
if (place_meeting(x,y + vsp,obj_wall))
{
    while(!place_meeting(x,y + sign(vsp),obj_wall))
   {
        y += sign(vsp);
    }
    vsp = 0
}
y += vsp;

//face check for animations
if (hspeed &gt; 0) {
    global.face = 1; //right
} else if (hspeed &lt; 0) {
    global.face = 0; //left
}

if (key_escape = 1) {
    game_restart();
}
</string>
