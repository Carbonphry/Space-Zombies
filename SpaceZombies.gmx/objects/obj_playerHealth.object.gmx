        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Initalize Variables
grav = 0.5;
vsp = 0;
hsp = 0;
jumpspeed = 7;
movespeed = 0.3;
key_down = 0;
maxmovespeed = 3;
global.face = 1;
global.player_health = 100;
maxstamina = 100;
is_sprinting = false;
ifledging = false;
is_dodging = false;
is_hit = false;
attacking = 0;
x = obj_player.x
y = obj_player.y


</string>
