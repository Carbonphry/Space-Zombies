if (global.face = 1) 
    {
    image_xscale = 1;
    }
else if (global.face = 0) 
    {
    image_xscale = -1;
    }

    
if (keyboard_check(ord('D')) &amp;&amp; hspeed &lt; 0.5 &amp;&amp; hspeed &gt; 0)
{
    sprite_index = spr_player_turn;
}

else if (keyboard_check_pressed(ord('A')) &amp;&amp; hspeed &gt; -0.5 &amp;&amp; hspeed &lt; 0)
{
    sprite_index = spr_player_turn;
}
    
if (hspeed = 0)
    {
    sprite_index = spr_player_idle;
    }
    
else if (hspeed != 0)
    {
    sprite_index = spr_player_walk;
    image_speed = 0.15;
    }

    
if (hspeed &gt; 1 &amp;&amp; hspeed &lt;= 3 || hspeed &lt; -1 &amp;&amp; hspeed &gt;= -3)
    {
    sprite_index = spr_player_sprint;
    image_speed = 0.15;
    }

    
if (vsp &lt; 0)
    {
    sprite_index = spr_player_jump;
    }
    
else if (vsp &gt;= 0 &amp;&amp; (!place_meeting(x, y+1, obj_wall)))
    {
    sprite_index = spr_player_fall;
    }
</string>
