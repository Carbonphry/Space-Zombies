//Vertical Collision
if (place_meeting(x,y + vsp,obj_wall))
{
    while(!place_meeting(x,y + sign(vsp),obj_wall))
   {
        y += sign(vsp);
    }
    vsp = 0
}
y += vsp;

//face check for animations
if (hspeed &gt; 0) {
    global.face = 1; //right
} else if (hspeed &lt; 0) {
    global.face = 0; //left
}
